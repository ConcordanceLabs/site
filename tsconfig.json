{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "strictNullChecks": true,
    "jsx": "react-jsx",
    "types": ["astro/client"],
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@src/*": ["src/*"],
      "@components/*": ["./src/components/*"],
      "@content/*": ["./src/content/*"],
      "@layouts/*": ["./src/layouts/*"],
      "@pages/*": ["./src/pages/*"],
      "@support/*": ["./src/support/*"],
      "@images/*": ["./src/images/*"]
    }
  }
}
